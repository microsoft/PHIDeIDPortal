{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "Domain": "{PLACEHOLDER}",
    "TenantId": "{PLACEHOLDER}",
    "ClientId": "{PLACEHOLDER}",
    "CallbackPath": "/signin-oidc",
    "SignedOutCallbackPath": "/signout-callback-oidc"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Cosmos": "{PLACEHOLDER}"
  },
  "CosmosDb": {
    "Endpoint": "https://{PLACEHOLDER}.documents.azure.com:443/",
    "DatabaseId": "deid",
    "ContainerId": "metadata",
    "PartitionKey": "/Uri",
    "UseEntraAuth": false
  },
  "SearchService": {
    "Uri": "https://{PLACEHOLDER}.search.windows.net",
    "ApiKey": "{PLACEHOLDER}",
    "IndexName": "piiredaction",
    "DefaultIndexerName": "piiredaction-indexer",
    "ReindexOnUpload": "true"
  },
  "StorageAccount": {
    "Uri": "https://{PLACEHOLDER}.blob.core.windows.net",
    "Container": "datamover-uploads",
    "UseManagedIdentity": true,
    "AccountName": "",
    "AccountKey": ""
  },
  "ApplicationInsights": {
    "ConnectionString": "{PLACEHOLDER}"
  },
  "FeatureManagement": {
    "Upload": true,
    "Search": true,
    "Delete": true,
    "Download": true,
    "ResetIndex": true,
    "Reindex": true,
    "TabbedNavigation": true,
    "AllDocumentsView": true,
    "UnprocessedView": true,
    "JustificationView": true,
    "ManualReviewView": true,
    "ApprovedView": true,
    "DeniedView": true
  },
  "Kestrel": {
    "MaxRequestBodySizeinMB": 50
  },
  "GroupClaimAdminId": "{PLACEHOLDER}"
}